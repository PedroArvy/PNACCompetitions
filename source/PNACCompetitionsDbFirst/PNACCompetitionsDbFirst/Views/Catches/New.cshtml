@using PNACCompetitionsDbFirst.Models.ViewModels
@model CatchEdit

@{
  ViewBag.Title = "New Catch";
}



<div class="row">
  <div class="col-md-8">
    <h2>New Catch</h2>
  </div>
</div>


<div class="row">
  <div class="col-md-10">

    @Html.ValidationSummary()

    @using (Html.BeginForm("New", "Catches", FormMethod.Post, new { @class = "form-horizontal", role = "form", id = "CatchEdit" }))
    {
      @Html.AntiForgeryToken()
      @Html.EditorFor(r => r)
    }

  </div>
</div>

@section Scripts {
  @Scripts.Render("~/bundles/jqueryval")
}

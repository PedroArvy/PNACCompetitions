@using PNACCompetitionsDbFirst.Models.ViewModels
@model ResultEdit


@Html.AntiForgeryToken()

<div class="form-group">
  <div class="col-md-10">
    @Html.LabelFor(m => m.Name, new { @class = "pnac-label" })
  </div>
  <div class="col-md-10">
    @Html.TextBoxFor(m => m.Name, new { @class = "form-control" })
    @Html.ValidationMessageFor(m => m.Name)
  </div>
</div>


<div class="form-group">
  <div class="col-md-10">
    <input type="submit" class="btn btn-default save-button" value="Save" />
    <a href="/fish/index" class="btn btn-default save-button back-button">Back to all results</a>
  </div>
</div>


@using PNACCompetitionsDbFirst.Models.ViewModels
@model FishEdit

@{
  ViewBag.Title = "New Fish";
}

<div class="row">
  <div class="col-md-8">
    <h2>New Fish</h2>
  </div>
</div>


<div class="row">
  <div class="col-md-10">

    @using (Html.BeginForm("New", "Fish", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
    {
      @Html.AntiForgeryToken()

      @Html.EditorFor(m => m)
    }

  </div>
</div>

@section Scripts {
  @Scripts.Render("~/bundles/jqueryval")
}


